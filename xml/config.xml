<?xml version="1.0" encoding="utf-8"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="testing.test.coed" version="1.0.0">
  <name>testing</name>
  <description>
        A basic Framework7 template for PhoneGap.
    </description>
  <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <preference name="android-minSdkVersion" value="14"/>
    <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
    <preference name="BackupWebStorage" value="none"/>
  </platform>
  <preference name="DisallowOverscroll" value="true"/>
  <plugin name="cordova-plugin-console" spec="~1.0.1"/>
  <plugin name="cordova-plugin-statusbar" spec="~1.0.1"/>
  <plugin name="cordova-plugin-whitelist" spec="~1.2.2"/>
  <preference name="WindowsStorePublisherName" value="My Name"/>
  <preference name="WindowsStoreIdentityName" value="d3309f5a-fc16-a512-3a92-e02919e5b590"/>
</widget>
